{
  "name": "pfa-next",
  "version": "1.0.0",
  "main": "index.js",
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/",
      "dist/",
      "coverage/"
    ],
    "watch": [
      "src/"
    ],
    "execMap": {
      "ts": "node -r ts-node/register"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "devnodemon": "nodemon src/server.ts",
    "devnodemon:debug": "nodemon --inspect-brk src/server.ts",
    "devalt:server": "nodemon --watch './**/*.ts' --exec 'ts-node' src/server.ts",
    "server": "nodemon src/server.js",
    "client": "npm start --prefix pfa-client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "tsc --project ./",
    "test": "jest --config ./jest.config.json"
  },
  "repository": "",
  "author": "aestheticsdata <aesthetics.data@gmail.com>",
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@sendgrid/mail": "^7.6.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "generate-password": "^1.7.0",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "nan": "^2.15.0",
    "prisma": "^4.8.0",
    "sib-api-v3-sdk": "^8.5.0",
    "ssh2": "^1.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "nodemon": "^2.0.15"
  }
}
